{
  "manifest_version": 3,
  "name": "Video Playback Controller",
  "version": "1.0",
  "description": "Control video playback with custom controls.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon.png",
      "32": "icons/icon.png",
      "48": "icons/icon.png",
      "128": "icons/icon.png"
    }
  }
}